// SSL 3.0 - Functional Collections Demo
// Demonstrates immutable update methods

// Map operations (functional style)
let config = {"host": "localhost", "port": "8080"}

// map_with creates a NEW map with updated value
let config2 = map_with(config, "host", "0.0.0.0")
let config3 = map_with(config2, "debug", "true")

// Original unchanged
let orig_host = config["host"]

// New map has updates
let new_host = config3["host"]
let debug = config3["debug"]

return new_host
