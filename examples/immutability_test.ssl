// SSL 3.0 - Immutability and Auto-Currying Test

// Test 1: Mutable variable assignment
let mut x = 10
x = 20
if x != 20 {
    print("Error: Mutable assignment failed")
    return 1
}

// Test 2: Mutable map index assignment
let mut map = {"key": "old"}
map["key"] = "new"
if map["key"] != "new" {
    print("Error: Mutable map update failed")
    return 2
}

// Test 3: Auto-Currying
fn add(a: Int, b: Int) -> Int {
    return a + b
}

// Call with fewer args -> should return partial function
let add10 = add(10)
let result = add10(5)

if result != 15 {
    print("Error: Auto-currying failed, got: " + result)
    return 3
}

print("Success: Mutable operations and Auto-Currying working!")
return 0
