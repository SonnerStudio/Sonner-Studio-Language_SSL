// Data Processing Pipeline
// Demonstrates Maps, JSON, and data transformation

fn main() {
    print("ğŸ“Š Data Processing Pipeline")
    print("============================\n")
    
    // Create a configuration map
    let config = {
        "app_name": "SSL Data Processor",
        "version": "0.1.0",
        "features": ["json", "http", "parallel"],
        "active": true
    }
    
    print("âš™ï¸  Configuration:")
    print(config)
    
    // Convert to JSON
    let config_json = json_stringify(config)
    print("\nğŸ“ JSON representation:")
    print(config_json)
    
    // Save configuration
    fs_write("config.json", config_json)
    print("\nğŸ’¾ Config saved to config.json")
    
    // Load it back
    let loaded = fs_read("config.json")
    let parsed = json_parse(loaded)
    
    print("\nâœ… Config loaded successfully:")
    print(parsed)
    
    // Create sample data
    let users = [
        {"name": "Alice", "score": 95},
        {"name": "Bob", "score": 87},
        {"name": "Charlie", "score": 92}
    ]
    
    print("\nğŸ‘¥ User data:")
    for user in users {
        print("  -", user)
    }
    
    // Save users to file
    fs_write("users.json", json_stringify(users))
    print("\nğŸ’¾ Users saved to users.json")
    
    print("\nğŸ‰ Pipeline completed!")
}
