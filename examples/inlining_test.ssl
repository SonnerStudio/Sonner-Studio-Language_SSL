// SSL 3.0 - Function Inlining Test

// Small helper function - should be inlined
fn add(a: Int, b: Int) -> Int {
    return a + b
}

// Another small function
fn double(x: Int) -> Int {
    return x * 2
}

// Main computation using helper functions
fn compute() -> Int {
    let x = add(10, 20)      // Should be inlined to: let x = 10 + 20
    let y = double(x)         // Should be inlined to: let y = x * 2
    let z = add(y, 5)         // Should be inlined to: let z = y + 5
    return z
}

// Test it
let result = compute()
print("Result: ")
print(result)

// Expected: 65
// (10 + 20) = 30
// 30 * 2 = 60
// 60 + 5 = 65

if result == 65 {
    return 0
}

return 1
