
fn main() {
    println("Testing asset:// protocol with wry 0.35...");
    
    html {
        <div style="display: flex; justify-content: center; align-items: center; height: 100vh; background: linear-gradient(135deg, #667eea 0%, #764ba2 100%); color: white; flex-direction: column; font-family: 'Segoe UI', Tahoma, sans-serif;">
            <h1 style="font-weight: 300; font-size: 3rem; margin-bottom: 20px;">SSL v9 - File Protocol Test</h1>
            <p style="margin-bottom: 30px; font-size: 1.2rem;">Loading external image via asset:// protocol:</p>
            <img src="asset://localhost/ssl-v9/test_logo.png" alt="Test Logo" style="max-width: 300px; border-radius: 15px; box-shadow: 0 8px 16px rgba(0,0,0,0.3); background: white; padding: 20px;" onerror="this.style.display='none'; document.getElementById('error').style.display='block';" onload="document.getElementById('success').style.display='block';" />
